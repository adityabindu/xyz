2. Validate JavaScript the registration ,user login, user profile and payment by credit card pages using JavaScript.
<html>
<head>
<script>
function validateForm(){
var  fname=document.myForm.firstname.value;
var  lname=document.myForm.lastname.value;
var  email=document.myForm.email.value;
var  bday=document.myForm.birthday.value;
var  gender=document.myForm.gender.value;
if(fanme==""){
window.alert("please enter your name");
fname.focus();
return false;
}
if("lname==""){
alert("please enter last name");
return false;
}
if("email==""){
alert("please enter birth day");
return false;
}
if("gender=="");
return false;
}
else{
alert("Thank you for your registration");
}}
</script>
</head>
<body>
<form name="myForm"submit="return validateForm()" method="post">
<h4>REGISTRATION FORM</h4>
<p><font color="red">*</font>Mandatory Fields</p>
<table>
<tr>
<td>
First Name:*
</td>
<td>
<input type="text" name="firstname"/>
</td></tr>
<tr>

<td>
Last Name:*
</td>
<td>
<input type="text" name="lastname"/>
</td>
</tr>
<tr>
Email:*
</td>
<td>
<input type="text" name="email"/>
</td></tr><tr><td>
Date of Birth:*
</td>
<td>
<input type="date" name="birthday"/>
</td></tr><tr><td>
Your Website:*
</td>
<td>
<input type="url" name="homepage" value=""/>
</td></tr><tr><td>
Gender:*
</td>
<td>
<select name="gender">
<option value="male">Male</option>
<option value="female">Female</option>
</select>
</td></tr><tr><td>
<input type="submit" name="reg value" Register""/>
</td></tr>
</table>
</form>
</body>
</html>





